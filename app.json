{
  "expo": {
    "name": "COMOT YAMA YAMA",
    "slug": "COMOT-YMA_YAA",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/Logistics.png",
    "userInterfaceStyle": "automatic",
    "scheme": "com.groovetech.comotyamayama",
    "splash": {
      "image": "./assets/Recycling.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.groovetech.comotyamayama",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.groovetech.comotyamayama",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/Recycling-p.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyARwk8fNFtc3D-qHecQamjUBMeI0pI5itU"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-build-properties",
      [
      "expo-image-picker",
      {
        "photosPermission": "The app accesses selected photo to share with PSP vendor.",
        "cameraPermission" : "Allow COMOT YAMA YAMA to access your camera"
      }
      ],
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
          // devTeam: "91SW8A37CR"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "fd236a7d-fe1f-4612-86e9-bda196f44f62"
      },
      "oneSignalAppId": "50da044d-9f6d-420c-a896-cb560f9f9bee"
    }
  }
}
