{
  "expo": {
    "name": "COMOT-YMA_YAA",
    "slug": "COMOT-YMA_YAA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "auto",
    "scheme": "com.groovetech.comot_yamayama",
    "splash": {
      "image": "./assets/bottles-anim.gif",
      "resizeMode": "cover",
      "backgroundColor": "#25292e"
